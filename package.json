{
  "name": "web-app",
  "private": true,
  "scripts": {
    "build-functions": "cd functions && pnpm build",
    "build-functions-watch": "cd functions && pnpm build-watch",
    "next-dev": " next dev & pnpm run start-emulators",
    "start-emulators": "pnpm run build-functions-watch & firebase emulators:start --import=./dir --export-on-exit=./dir",
    "dev": "pnpm kill-port 8080 9909 4000 3000 5001 && pnpm run next-dev",
    "analyze": "ANALYZE=true next build",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "style:all": "tsc && pnpm run style:lint && pnpm run style:prettier",
    "style:lint": "eslint '**/*.{js,jsx,ts,tsx}' --ignore-path .gitignore",
    "style:prettier": "prettier --check '**/*.{js,jsx,ts,tsx}' --ignore-path .gitignore",
    "make-pretty": "prettier --write '**/*.{js,jsx,ts,tsx}' --ignore-path .gitignore",
    "import-from-prod": "gsutil rm -r gs://urm-app.appspot.com/firestore_export && gcloud firestore export gs://urm-app.appspot.com/firestore_export && gsutil -m cp -r gs://urm-app.appspot.com/firestore_export ./dir"
  },
  "dependencies": {
    "@algolia/cache-in-memory": "^4.14.2",
    "@algolia/client-search": "^4.14.2",
    "@emotion/react": "^11.10.4",
    "@emotion/styled": "^11.10.4",
    "@firebase/app-types": "^0.8.0",
    "@mui/icons-material": "^5.10.9",
    "@mui/material": "^5.10.9",
    "@mui/system": "^5.10.9",
    "@mui/utils": "^5.10.9",
    "@mui/x-date-pickers": "^5.0.4",
    "algoliasearch": "^4.14.2",
    "classnames": "^2.3.2",
    "date-fns": "^2.29.3",
    "dompurify": "^2.4.1",
    "firebase": "^9.12.0",
    "firebase-admin": "^11.1.0",
    "firebase-tools": "^11.14.2",
    "next": "^12.3.1",
    "nextjs-progressbar": "^0.0.16",
    "nookies": "^2.5.2",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-dropzone": "^14.2.2",
    "react-easy-crop": "^4.6.2",
    "react-firebase-hooks": "^5.0.3",
    "react-image-crop": "^10.0.8",
    "react-infinite-scroll-component": "^6.1.0",
    "sharp": "^0.31.1",
    "tmp": "^0.2.1",
    "uuid": "^9.0.0"
  },
  "devDependencies": {
    "@babel/core": "^7.19.3",
    "@next/bundle-analyzer": "^12.3.1",
    "@next/eslint-plugin-next": "^12.3.1",
    "@types/dompurify": "^2.4.0",
    "@types/lodash": "^4.14.186",
    "@types/node": "^17.0.45",
    "@types/react": "^17.0.50",
    "@types/react-dom": "^18.0.6",
    "@types/styled-components": "^5.1.26",
    "@types/tmp": "^0.2.3",
    "@types/uuid": "^8.3.4",
    "@typescript-eslint/eslint-plugin": "^5.40.0",
    "@typescript-eslint/parser": "^5.40.0",
    "eslint": "^7.32.0",
    "eslint-config-next": "^12.3.1",
    "eslint-config-prettier": "^8.5.0",
    "eslint-config-standard": "^16.0.3",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-jsx-a11y": "^6.6.1",
    "eslint-plugin-mui": "^0.1.1",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-promise": "^5.2.0",
    "eslint-plugin-react": "^7.31.10",
    "eslint-plugin-react-hooks": "^4.6.0",
    "kill-port": "^2.0.1",
    "prettier": "^2.7.1",
    "prop-types": "^15.8.1",
    "typescript": "^4.8.4"
  }
}
