{
  "name": "web-app",
  "private": true,
  "scripts": {
    "build-functions": "cd functions && pnpm build",
    "build-functions-watch": "cd functions && pnpm build-watch",
    "next-dev": " next dev & pnpm run start-emulators",
    "start-emulators": "pnpm run build-functions-watch & firebase emulators:start --import=./dir",
    "dev": "pnpm kill-port 8080 9099 4000 3000 5001 9000 && pnpm run next-dev",
    "analyze": "ANALYZE=true next build",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "style:all": "tsc && pnpm run style:lint && pnpm run style:prettier",
    "style:lint": "eslint '**/*.{js,jsx,ts,tsx}' --ignore-path .gitignore",
    "style:prettier": "prettier --check '**/*.{js,jsx,ts,tsx}' --ignore-path .gitignore",
    "make-pretty": "prettier --write '**/*.{js,jsx,ts,tsx}' --ignore-path .gitignore",
    "import-from-prod": "gsutil rm -r gs://urm-app.appspot.com/firestore_export && gcloud firestore export gs://urm-app.appspot.com/firestore_export && gsutil -m cp -r gs://urm-app.appspot.com/firestore_export ./dir"
  },
  "dependencies": {
    "@algolia/cache-in-memory": "^4.17.0",
    "@algolia/client-search": "^4.17.0",
    "@emotion/react": "^11.10.6",
    "@emotion/styled": "^11.10.6",
    "@firebase/app-types": "^0.9.0",
    "@mui/icons-material": "^5.11.16",
    "@mui/material": "^5.11.16",
    "@mui/system": "^5.11.16",
    "@mui/utils": "^5.11.13",
    "@mui/x-date-pickers": "^5.0.20",
    "algoliasearch": "^4.17.0",
    "classnames": "^2.3.2",
    "date-fns": "^2.29.3",
    "dompurify": "^3.0.1",
    "firebase": "^9.22.1",
    "firebase-admin": "^11.6.0",
    "firebase-tools": "^11.30.0",
    "next": "^13.3.0",
    "nextjs-progressbar": "^0.0.16",
    "nookies": "^2.5.2",
    "react": "^18.2.0",
    "react-device-detect": "^2.2.3",
    "react-dom": "^18.2.0",
    "react-dropzone": "^14.2.3",
    "react-easy-crop": "^4.7.4",
    "react-error-boundary": "^4.0.3",
    "react-firebase-hooks": "^5.1.1",
    "react-image-crop": "^10.0.9",
    "react-infinite-scroll-component": "^6.1.0",
    "react-social-login-buttons": "^3.9.1",
    "sharp": "^0.31.3",
    "tmp": "^0.2.1",
    "uuid": "^9.0.0"
  },
  "devDependencies": {
    "@babel/core": "^7.21.4",
    "@mui/types": "^7.2.4",
    "@next/bundle-analyzer": "^13.3.0",
    "@next/eslint-plugin-next": "^13.3.0",
    "@types/dompurify": "^2.4.0",
    "@types/lodash": "^4.14.192",
    "@types/node": "^18.15.11",
    "@types/react": "^18.0.33",
    "@types/react-dom": "^18.0.11",
    "@types/styled-components": "^5.1.26",
    "@types/tmp": "^0.2.3",
    "@types/uuid": "^9.0.1",
    "@typescript-eslint/eslint-plugin": "^5.57.1",
    "@typescript-eslint/parser": "^5.57.1",
    "eslint": "^8.38.0",
    "eslint-config-next": "^13.3.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-config-standard": "^17.0.0",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-jsx-a11y": "^6.7.1",
    "eslint-plugin-mui": "^0.1.1",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-promise": "^6.1.1",
    "eslint-plugin-react": "^7.32.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "kill-port": "^2.0.1",
    "prettier": "^2.8.7",
    "prop-types": "^15.8.1",
    "typescript": "^4.9.5"
  }
}
