{
  "name": "web-app",
  "private": true,
  "scripts": {
    "build-functions": "cd functions && pnpm build",
    "build-functions-watch": "cd functions && pnpm build-watch",
    "next-dev": " next dev & pnpm run start-emulators",
    "start-emulators": "pnpm run build-functions-watch & firebase emulators:start --import=./dir",
    "dev": "pnpm kill-port 8080 9099 4000 3000 5001 9000 && pnpm run next-dev",
    "analyze": "ANALYZE=true next build",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "style:all": "tsc && pnpm run style:lint && pnpm run style:prettier",
    "style:lint": "eslint '**/*.{js,jsx,ts,tsx}' --ignore-path .gitignore",
    "style:prettier": "prettier --check '**/*.{js,jsx,ts,tsx}' --ignore-path .gitignore",
    "make-pretty": "prettier --write '**/*.{js,jsx,ts,tsx}' --ignore-path .gitignore",
    "import-from-prod": "gsutil rm -r gs://urm-app.appspot.com/firestore_export && gcloud firestore export gs://urm-app.appspot.com/firestore_export && gsutil -m cp -r gs://urm-app.appspot.com/firestore_export ./dir",
    "test": "jest --forceExit test/test.ts"
  },
  "dependencies": {
    "@algolia/cache-in-memory": "^4.22.1",
    "@algolia/client-search": "^4.22.1",
    "@emotion/react": "^11.11.3",
    "@emotion/styled": "^11.11.0",
    "@firebase/app-types": "^0.9.0",
    "@mui/icons-material": "^5.15.4",
    "@mui/material": "^5.15.4",
    "@mui/system": "^5.15.4",
    "@mui/utils": "^5.15.4",
    "@mui/x-date-pickers": "^5.0.20",
    "algoliasearch": "^4.22.1",
    "classnames": "^2.5.1",
    "date-fns": "^2.30.0",
    "dompurify": "^3.0.8",
    "firebase": "^10.7.1",
    "firebase-admin": "^12.0.0",
    "instantsearch.js": "^4.63.1",
    "next": "^13.5.6",
    "nextjs-progressbar": "^0.0.16",
    "nookies": "^2.5.2",
    "react": "^18.2.0",
    "react-animate-height": "^3.2.3",
    "react-device-detect": "^2.2.3",
    "react-dom": "^18.2.0",
    "react-dropzone": "^14.2.3",
    "react-easy-crop": "^4.7.5",
    "react-error-boundary": "^4.0.12",
    "react-firebase-hooks": "^5.1.1",
    "react-image-crop": "^10.1.8",
    "react-infinite-scroll-component": "^6.1.0",
    "react-instantsearch": "^7.5.1",
    "react-social-login-buttons": "^3.9.1",
    "sharp": "^0.31.3",
    "tmp": "^0.2.1",
    "uuid": "^9.0.1"
  },
  "devDependencies": {
    "@babel/core": "^7.23.7",
    "@firebase/firestore-types": "^3.0.0",
    "@firebase/rules-unit-testing": "^3.0.1",
    "@jest/globals": "^29.7.0",
    "@mui/types": "^7.2.13",
    "@next/bundle-analyzer": "^13.5.6",
    "@next/eslint-plugin-next": "^13.5.6",
    "@types/dompurify": "^2.4.0",
    "@types/jest": "^29.5.11",
    "@types/lodash": "^4.14.202",
    "@types/node": "^18.19.7",
    "@types/react": "^18.2.48",
    "@types/react-dom": "^18.2.18",
    "@types/styled-components": "^5.1.34",
    "@types/tmp": "^0.2.6",
    "@types/uuid": "^9.0.7",
    "@typescript-eslint/eslint-plugin": "^5.62.0",
    "@typescript-eslint/parser": "^5.62.0",
    "eslint": "^8.56.0",
    "eslint-config-next": "^13.5.6",
    "eslint-config-prettier": "^8.10.0",
    "eslint-config-standard": "^17.1.0",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-jsx-a11y": "^6.8.0",
    "eslint-plugin-mui": "^0.1.1",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-promise": "^6.1.1",
    "eslint-plugin-react": "^7.33.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "jest": "^29.7.0",
    "kill-port": "^2.0.1",
    "mocha": "^10.2.0",
    "prettier": "^2.8.8",
    "prop-types": "^15.8.1",
